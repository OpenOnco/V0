# OpenOnco API Keys for Evaluation
# Copy this file to .env and fill in your keys

# Required for chatbot evaluation
ANTHROPIC_API_KEY=sk-ant-...

# Optional: for multi-LLM rating
OPENAI_API_KEY=sk-...
GOOGLE_API_KEY=...

# PostHog Analytics (for product analytics)
# Get your key from: https://app.posthog.com/project/settings
VITE_POSTHOG_KEY=phc_...
VITE_POSTHOG_HOST=https://us.i.posthog.com

# Feature Flags
# Set to 'true' to use the new tool-based chat-v2 endpoint
VITE_USE_CHAT_V2=false
