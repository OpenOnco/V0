{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/v1/**/*.js": {
      "includeFiles": "src/data.js"
    }
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "openonco.org"
        }
      ],
      "destination": "https://www.openonco.org/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/"
    },
    
    {
      "source": "/mrd/:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/mrd/:slug*"
    },
    {
      "source": "/ecd/:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/ecd/:slug*"
    },
    {
      "source": "/trm/:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/trm/:slug*"
    },
    {
      "source": "/tds/:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/tds/:slug*"
    },
    {
      "source": "/cgp/:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/cgp/:slug*"
    },
    {
      "source": "/test/:slug*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/test/:slug*"
    },
    
    {
      "source": "/mrd",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/mrd"
    },
    {
      "source": "/ecd",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/ecd"
    },
    {
      "source": "/trm",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/trm"
    },
    {
      "source": "/tds",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/tds"
    },
    {
      "source": "/cgp",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Facebot|Twitterbot|LinkedInBot|Slackbot|WhatsApp|TelegramBot|Discordbot|Applebot|Pinterest|vkShare|Googlebot|bingbot|CFNetwork|Darwin|iMessageLinkPreview).*"
        }
      ],
      "destination": "/api/og?path=/cgp"
    },
    
    {
      "source": "/mrd",
      "destination": "/"
    },
    {
      "source": "/ecd",
      "destination": "/"
    },
    {
      "source": "/trm",
      "destination": "/"
    },
    {
      "source": "/cgp",
      "destination": "/"
    },
    {
      "source": "/submissions",
      "destination": "/"
    },
    {
      "source": "/submit",
      "destination": "/"
    },
    {
      "source": "/how-it-works",
      "destination": "/"
    },
    {
      "source": "/about",
      "destination": "/"
    },
    {
      "source": "/changelog",
      "destination": "/"
    },
    {
      "source": "/test/:path*",
      "destination": "/"
    },
    {
      "source": "/((?!api|_next|static|favicon|@|node_modules|.*\\..*).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/api/v1/embed/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=300"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/api/v1/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=600"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
